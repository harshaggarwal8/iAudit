<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="iAudit.Views.MonthsPage">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical">
            <Button Text="January" Clicked="Jan_Clicked"/>
            <Button Text="February" Clicked="Feb_Clicked"/>
            <Button Text="March" Clicked="Mar_Clicked"/>
            <Button Text="April" Clicked="Apr_Clicked"/>
            <Button Text="May" Clicked="May_Clicked"/>
            <Button Text="June" Clicked="Jun_Clicked"/>
            <Button Text="July" Clicked="Jul_Clicked"/>
            <Button Text="August" Clicked="Aug_Clicked"/>
            <Button Text="September" Clicked="Sep_Clicked"/>
            <Button Text="October" Clicked="Oct_Clicked"/>
            <Button Text="November" Clicked="Nov_Clicked"/>
            <Button Text="December" Clicked="Dec_Clicked"/>
            <Button Text="View All" Clicked="All_Clicked"/>
        </StackLayout>        
    </ContentPage.Content>
</ContentPage>
